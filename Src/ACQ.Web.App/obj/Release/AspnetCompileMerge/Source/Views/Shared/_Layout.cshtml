<!DOCTYPE html>
<html>
<head>
    <title>MOD (Acquisition) </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="utf-8">
    <!-- External CSS libraries -->
    <title>Acquisition Portal </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="utf-8">
    <!-- External CSS libraries -->
    <link href="~/assets/css/bootstrapv4.css" rel="stylesheet" />
    <link href="~/assets/css/style.css" rel="stylesheet" />
    <link href="~/assets/fontawesome-5.12.1/css/all.min.css" rel="stylesheet" />

    <script src="~/assets/js/jquery.min.js"></script>
</head>

<body>
  

    <div class="wrapper">
        <!-----Side Navigation------>
        <nav id="sidebar" class="sidebar" style="background:#1c2f61;">
            <div class="sidebar-content js-simplebar" style="background:#1c2f61;" data-simplebar="init">
                <div class="simplebar-wrapper" style="margin: 0px;">
                    <div class="simplebar-height-auto-observer-wrapper"><div class="simplebar-height-auto-observer"></div></div><div class="simplebar-mask">
                        <div class="simplebar-offset" style="right: 0px; bottom: 0px;">
                            <div class="simplebar-content-wrapper" style="height: 100%; overflow: hidden scroll;">
                                <div class="simplebar-content" style="padding: 0px;">
                                    @{ Html.RenderAction("imagelogo", "Account"); }

                                    <ul class="sidebar-nav">
                                        @if (Session["DepartmentID"].ToString() == "1")
                                        {
                                            <li class="sidebar-item">
                                                <a href="http://192.168.122.1:45455/Home/Index" class="sidebar-link collapsed" aria-expanded="false">
                                                    <span class="align-middle"> <i class="fas fa-tachometer-alt sidebar_icon"></i>Dashboard</span>
                                                </a>
                                            </li>
                                        }
                                        <li class="sidebar-item active">
                                            <a href="#pages" data-toggle="collapse" class="sidebar-link collapsed" aria-expanded="false">
                                                <svg xmlns="#" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-layout align-middle"><rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect><line x1="3" y1="9" x2="21" y2="9"></line><line x1="9" y1="21" x2="9" y2="9"></line></svg>
                                                <span class="align-middle">SoC</span>
                                            </a>

                                            <ul id="pages" class="sidebar-dropdown list-unstyled collapse" data-parent="#sidebar" style="">
                                                @if (Session["SectionID"].ToString() == "1" || Session["SectionID"].ToString() == "12" || Session["SectionID"].ToString() == "13")
                                                {
                                                    <li class="sidebar-item" id="mAccess">
                                                        <a class="sidebar-link" id="SoUp" href="@Url.Action("SoCPdfRegistration", "SocPdfRegistration")"><span><i class="fas fa-layer-group"></i>  SoC Upload</span></a>
                                                    </li>
                                                    <li class="sidebar-item" id="mPrintingUser"><a class="sidebar-link" href="@Url.Action("ViewSOCRegistration", "AoNW")"><span><i class="fas fa-layer-group"></i>  SoC View</span></a></li>
                                                    @*<li class="sidebar-item" id="mPrintingUser"><a class="sidebar-link" href="@Url.Action("ViewSOCRegistration", "AoNW")"><span><i class="fas fa-layer-group"></i>  Approval for sending SoC to DAC/DPB</span></a></li>*@
                                                }
                                                else
                                                {
                                                    <li class="sidebar-item" id="mPrintingUser"><a class="sidebar-link" href="@Url.Action("ViewSOCRegistration", "AoNW")"><span><i class="fas fa-layer-group"></i>  SoC View</span></a></li>

                                                }


                                            </ul>
                                        </li>


                                        <li class="sidebar-item active">
                                            <a href="#mom" data-toggle="collapse" class="sidebar-link collapsed" aria-expanded="false">
                                                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-layout align-middle"><rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect><line x1="3" y1="9" x2="21" y2="9"></line><line x1="9" y1="21" x2="9" y2="9"></line></svg>
                                                <span class="align-middle">MoM</span>
                                            </a>
                                            <ul id="mom" class="sidebar-dropdown list-unstyled collapse" data-parent="#sidebar" style="">
                                                <li class="sidebar-item" id="view_meet">
                                                    <a class="sidebar-link" href="@Url.Action("ViewMeeting","AoNW")"><span><i class="fas fa-layer-group"></i>  View Meeting</span></a>
                                                </li>
                                            </ul>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div><div class="simplebar-placeholder" style="width: auto; height: 1061px;"></div>
                </div><div class="simplebar-track simplebar-horizontal" style="visibility: hidden;"><div class="simplebar-scrollbar simplebar-visible" style="width: 0px; display: none;"></div></div><div class="simplebar-track simplebar-vertical" style="visibility: visible;"><div class="simplebar-scrollbar simplebar-visible" style="height: 141px; transform: translate3d(0px, 0px, 0px); display: block;"></div></div>
            </div>
        </nav>


        <div class="main">
            <!-----Header Start----->
            <nav class="navbar navbar-expand navbar-light navbar-bg">
                <a class="sidebar-toggle">
                    <i class="hamburger align-self-center"></i>
                </a>
                <div class="container-fluid">
                    <div class="row w-100">
                        <div class="col-md-3 d-flex align-items-center">
                            <label class="userlbl mb-0"><i class="fas fa-user-cog"></i> Welcome :- @Session["UserName"].ToString()</label>
                        </div>
                        <div class="col-md-6 headettxt">
                            ACQUISITION PORTAL
                        </div>
                        <div class="col-md-3 pr-0 d-flex justify-content-end align-items-center">
                            <div class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle d-none d-sm-inline-block" href="#" data-toggle="dropdown" aria-expanded="false">
                                    <i class="fas fa-user-alt mr-2"></i>My Profile

                                </a>
                                <div class="dropdown-menu dropdown-menu-right">
                                    <a href="/Account/ResetPwd" class="dropdown-item"><i class="fas fa-lock mr-2"></i>Change Password</a>
                                    <a href="/Account/Logout" class="dropdown-item"><i class="fas fa-sign-out-alt mr-2"></i> Logout</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>

            <main class="content">
                @RenderBody()
            </main>

            <!-------------footer start-------------->
            <footer class="bg-dark footer-bx">
                <div class="bg-darker">
                    <div class="container">
                        <div class="pb-2 pt-3 fnt-s text-light opacity-80 text-center">
                            © All rights reserved. 2021
                            <a class="text-light" href="#" target="_blank" rel="noopener"> By Acquisition Portal</a>
                        </div>
                    </div>
                </div>
            </footer>
            <!-------------footer end-------------->
        </div>
    </div>



    <script src="~/assets/js/app.js"></script>
    <script src="~/assets/fontawesome-5.12.1/js/all.min.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $('#sidebarCollapse').on('click', function () {
                $('#sidebar').toggleClass('active');
            });
        });
    </script>
</body>
</html>
<script>
    $(document).ready(function () {
        function disableBack() { window.history.forward() }

        window.onload = disableBack();
        window.onpageshow = function (evt) { if (evt.persisted) disableBack() }

        window.history.pushState(null, "", window.location.href);
        window.onpopstate = function () {
            window.history.pushState(null, "", window.location.href);
        };

        $("body").on("contextmenu", function (e) {
            return false;
        });

        $(window).keydown(function (event) {

            if (event.keyCode == 116) {

                event.preventDefault();

                return false;

            }

        });
    });
</script>






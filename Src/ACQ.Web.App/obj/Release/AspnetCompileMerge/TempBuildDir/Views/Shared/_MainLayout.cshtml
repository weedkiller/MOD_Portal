<!DOCTYPE html>
<html>
<head>
    <title>Acquisition Portal </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="utf-8">
    <!-- External CSS libraries -->
    <link href="~/assets/css/bootstrapv4.css" rel="stylesheet" />
        <link href="~/assets/css/style.css" rel="stylesheet" />
        <link href="~/assets/fontawesome-5.12.1/css/all.min.css" rel="stylesheet" />
    @*@Styles.Render("~/Contentnew/css")*@

    <script src="~/assets/js/jquery.min.js"></script>
</head>
<body>

    <!-------------header start-------------->
    <div class="navbar-custom topnav-navbar topnav-navbar-dark" style="height: 95px;">
        <div class="container-fluid">
            <div class="row">
                <div class="col-2">
                    <a href="" class="topnav-logo mt-2">
                        <span class="topnav-logo-sm">
                            @{ Html.RenderAction("imagelogo"); }
                        </span>
                    </a>
                </div>

                <div class="col-8 d-flex justify-content-center align-items-center">
                    <h1 class="mt-3 topHeadline">ACQUISITION PORTAL</h1>
                </div>

                <div class="col-2">

                </div>
            </div>
        </div>
    </div>

    <div class="body_content">
        @RenderBody()
    </div>
    <!-------------footer start-------------->
    <footer class="bg-dark">
        <div class="container">
            <div class="pb-2 pt-3 fnt-s text-light opacity-80 text-center">
                © All rights reserved. 2021
                <a class="text-light" href="#" target="_blank" rel="noopener"> By Acquisition Portal</a>
            </div>
        </div>
    </footer>
    <!-------------footer end-------------->
    @*@Scripts.Render("~/bundles/js")*@

    <script src="~/assets/js/app.js"></script>
        <script src="~/assets/fontawesome-5.12.1/js/all.min.js"></script>

</body>
</html>

<script>
    $(document).ready(function () {
        function disableBack() { window.history.forward() }

        window.onload = disableBack();
        window.onpageshow = function (evt) { if (evt.persisted) disableBack() }

        window.history.pushState(null, "", window.location.href);
        window.onpopstate = function () {
            window.history.pushState(null, "", window.location.href);
        };

        $("body").on("contextmenu", function (e) {
            return false;
        });

        $(window).keydown(function (event) {

            if (event.keyCode == 116) {

                event.preventDefault();

                return false;

            }

        });
    });
</script>

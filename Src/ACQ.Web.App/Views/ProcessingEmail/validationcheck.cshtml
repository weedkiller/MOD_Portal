    <script nonce="r@nd0m">
    jQuery(document).ready(function ($) {
        document.title = 'Proactive alert Report';
        $("#FromDate").datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: 'dd/mm/yy'

        });


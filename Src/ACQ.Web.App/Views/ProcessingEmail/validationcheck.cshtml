                            var l2;
                            var l3;
                            var l4;
                            var l5;
                            var l6;
                            var l7;
                            var l8;
                            var l9;

                            row += '<td>';
                            if (item.L1_Officer_Email != "" && item.L1_Officer_Email != "N/A") {
                                l1 = '<div class="cards"> Name : ' + item.L1_Officer_Name + ' (' + item.L1_Designation + ') ' +' Phone: <span>' + item.L1_Officer_Phone + '</span>Email: <span>' + item.L1_Officer_Email + '</span></div>';
                                row += l1;
                            }
                            if (item.L2_Officer_Email != "" && item.L2_Officer_Email != "N/A") {
                                l2 = '<div class="cards"> Name : ' + item.L2_Officer_Name + ' (' + item.L2_Designation + ') ' +' Phone: <span>' + item.L2_Officer_Phone + '</span>Email: <span>' + item.L2_Officer_Email + '</span></div>';
                                row += l2;
                            }
                            if (item.L3_Officer_Email != "" && item.L3_Officer_Email != "N/A") {
                                l3 = '<div class="cards"> Name : ' + item.L3_Officer_Name + ' (' + item.L3_Designation + ') ' +' Phone: <span>' + item.L3_Officer_Phone + '</span>Email: <span>' + item.L3_Officer_Email + '</span></div>';
                                row += l3;
                            }
